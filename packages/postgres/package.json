{
  "name": "@evalstudio/postgres",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.2.0",
  "description": "PostgreSQL storage backend for EvalStudio",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "keywords": [
    "evalstudio",
    "postgres",
    "storage"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Treatwell-AI/evalstudio.git",
    "directory": "packages/postgres"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "pg": "^8.13.1",
    "@evalstudio/core": "workspace:*"
  },
  "devDependencies": {
    "@types/pg": "^8.11.11",
    "@types/node": "^22.10.10",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4"
  },
  "engines": {
    "node": ">=20"
  },
  "license": "MIT"
}
